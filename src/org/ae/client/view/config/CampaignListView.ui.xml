<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.important {
			font-size: x-large;
			padding: 10px
		}
	</ui:style>
	<g:HTMLPanel width="">
		<table width="100%">
			<tr>
				<td>
					<g:Label styleName='{style.important}' text="Campaigns" />
				</td>
				<td>
					<table valign="top" align="right">
						<tr>
							<td>
								<g:Button ui:field="btnNewCampaingn">Add</g:Button>
							</td>
							<td>
								<g:Button ui:field="btnEditCampaign">Edit</g:Button>
							</td>
							<td>
								<g:Button ui:field="btnDeleteCampaingn">Delete</g:Button>
							</td>
							<td>
								<g:Button ui:field="btnCopy">Copy</g:Button>
							</td>
						</tr>
					</table>
				</td>
			</tr>
			<tr>
				<td colspan="2">
					<p1:CellTable ui:field="cellTable" width="100%" />
				</td>
			</tr>
		</table>
		<table height="60px">
			<tr>
				<td>&nbsp;</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 